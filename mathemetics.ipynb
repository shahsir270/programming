    "def count_digits(n):\n",
    "    count = 0\n",
    "    while n > 0:\n",
    "        n = n//10\n",
    "        count += 1\n",
    "    return count\n",
    "\n",
    "print(count_digits(100))\n",
    "print(count_digits(15314))"
   ]
